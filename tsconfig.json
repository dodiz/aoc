{
  "display": "Node LTS",
  "_version": "18.1.0",
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
