{
  "display": "Node LTS",
  "_version": "18.1.0",
  "compilerOptions": {
    "lib": ["ESNext"],
    "outDir": "dist",
    "module": "ESNext",
    "target": "ES2015",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["src"]
}
